{
  "name": "CryptoBingo",
  "version": "1.0.0",
  "description": "A bingo card generator for cryptocurrency and blockchain topics. #cryptobingo",
  "license": "MIT",
  "author": "Jordan Mack <jordan.mack@brilliantnotion.com> (http://www.brilliantnotion.com/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/BlockSafari/cryptobingo"
  },
  "main": "index.html",
  "scripts": {
    "build": "npm run webpack-build-prod",
    "clean": "rm -f dist/bundle.css dist/bundle.js || del dist\\bundle.css dist\\bundle.js",
    "start": "npm run clean && npm run webpack-server-dev-hot",
    "webpack-build-dev": "node ./node_modules/webpack/bin/webpack.js --progress --colors --config dev/webpack/webpack.config.development.js",
    "webpack-build-prod": "node ./node_modules/webpack/bin/webpack.js --progress --colors --config dev/webpack/webpack.config.production.js",
    "webpack-server-dev-hot": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --disableHostCheck --host 0.0.0.0 --content-base dist --config dev/webpack/webpack.config.devserver.js --hot",
    "webpack-server-dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --disableHostCheck --host 0.0.0.0 --content-base dist --config dev/webpack/webpack.config.development.js",
    "webpack-server-prod": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --disableHostCheck --host 0.0.0.0 --content-base dist --config dev/webpack/webpack.config.production.js"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "html2canvas": "^0.5.0-beta4",
    "include-media": "^1.4.9",
    "lodash": "^4.14.2",
    "loglevel": "^1.4.1",
    "number-to-locale-string": "^1.0.1",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router-dom": "^4.1.2",
    "seedrandom": "^2.4.3",
    "setimmediate": "^1.0.4",
    "uuid": "^3.1.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "latest",
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "css-loader": "^0.27.2",
    "eslint": "^3.17.1",
    "eslint-plugin-flowtype": "^2.30.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.3",
    "react-addons-perf": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.3",
    "sort-package-json": "^1.5.0",
    "style-loader": "^0.13.2",
    "uglifyjs-webpack-plugin": "^0.3.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "engine": "node 7.5.0"
}
